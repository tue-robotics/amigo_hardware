<launch>
  <include file="$(find rtt_ros)/launch/deployer.launch">
    <arg name="DEPLOYER_ARGS" value="gnulinux
    -s $(find amigo_hardware)/soem.ops
    -s $(find amigo_hardware)/base_controller.ops
    -s $(find amigo_hardware)/spindle_controller.ops
    -s $(find amigo_hardware)/left_arm_controller.ops
    -s $(find amigo_hardware)/right_arm_controller.ops
    -s $(find amigo_hardware)/head_controller.ops
    "/>
    <arg name="LOG_LEVEL" value="Warning"/>
    <arg name="DEBUG" value="false"/>
  </include>
</launch>
